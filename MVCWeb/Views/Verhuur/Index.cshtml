@model IEnumerable<Genre>

<h1>Genres</h1>

<table class="table table-striped">
<tbody>
    <tr>
            @foreach(var item in Model)
            {
                <td>
                    <a asp-action="Details" asp-route-id="@item.GenreId">@item.GenreNaam</a>
                </td>
            }
        </tr>
</tbody>


</table>
